<Window x:Class="MT_Creator_WPF.colorChooser"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    Title="Color Chooser" Height="392" Width="300">
    <Grid>
        <UniformGrid Rows="16" Columns ="16" Margin="12,12,12,126" Name="uniformGrid1" />
        <Button Height="23" HorizontalAlignment="Right" Margin="0,0,12,12" Name="button1" VerticalAlignment="Bottom" Width="75" Click="button1_Click">OK</Button>
        <Button Height="23" Margin="110,0,93,12" Name="button2" VerticalAlignment="Bottom" Click="button2_Click">Cancel</Button>
        <Rectangle Height="59" Margin="0,0,12,51" Name="rectangle1" Stroke="Black" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="66" />
        <TextBlock Height="51" Margin="12,0,93,51" Name="textBlock1" VerticalAlignment="Bottom" Text="Selected Color" FontSize="16" />
    </Grid>
</Window>
