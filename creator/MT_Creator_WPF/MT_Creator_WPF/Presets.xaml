<Window x:Class="MT_Creator_WPF.Presets"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Presets" Height="151" Width="194">
    <Grid>
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="comboBox1" VerticalAlignment="Top" Width="120">
            <ComboBoxItem Content="Grass" DataContext="{Binding}" />
            <ComboBoxItem Content="Smooth Table" />
            <ComboBoxItem Content="Felt" />
            <ComboBoxItem Content="Sand" />
            <ComboBoxItem Content="Frictionless" />
            <ComboBoxItem Content="Ice" />
            <ComboBoxItem Content="Asphalt" />
        </ComboBox>
        <Button Content="OK" Height="20" HorizontalAlignment="Right" Margin="0,0,17,12" Name="Update" VerticalAlignment="Bottom" Width="67" />
        <Button Content="Cancel" Height="20" Margin="12,0,90,12" Name="Cancel" VerticalAlignment="Bottom" Click="Cancel_Click" />
    </Grid>
</Window>
